<!--Konten hasil-->

@{
    ViewData["Title"] = "Result";
}
<h2>@ViewData["Title"]</h2>

<p>Hasil pencarian dengan keyword = <strong>@ViewData["Keyword"]</strong> dan algoritma = 
    <strong>@ViewData["Algorithm"]</strong>.</p>

@if (ViewData["Algorithm"].ToString().Equals("Regex") || ViewData["Algorithm"].ToString().Equals("KMP") || ViewData["Algorithm"].ToString().Equals("Boyer-Moore"))
{
    var resultList = ViewData["Result"] as List<Dictionary<string, string>>;
    <ol>
        @foreach (var element in resultList)
        {
            <li>
                <a href=@element["link"]><b>@element["title"]</b></a>
                <br />
                <p>@element["match"]</p>
                <iframe width="600"
                        height="450"
                        frameborder="0" style="border:0"
                        src="https://www.google.com/maps/embed/v1/place?key=AIzaSyDLz3Txvxb6oeGYqZ_oZwg_qNrpSqJr6XU
                &q=@ViewData["Keyword"]" allowfullscreen></iframe>
            </li>
        }
    </ol>
}